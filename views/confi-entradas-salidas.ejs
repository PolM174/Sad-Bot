             <h2>
               Selecion canal entrada 1
            </h2>
             <form class="col s12" method="post" id="enviar_Welcome" action="/server/<%=guild.id%>/welcome1">
                <div class="row">
                  <div class="input-field col s6">
                    <i class="material-icons prefix">account_circle</i>
                    <select id="select-channel" name="channel_ID"> 
                      <%= login === 'si' ? '/perfil' : '/login' %>"
                      <option selected> Canal actual : <%= entrada1 %> </option>    
                      <option>Apagado</option>
                      <% guild.channels.cache.filter(ch => ch.type === 'text').filter(p => p.permissionsFor(client.user).has('SEND_MESSAGES') && p.permissionsFor(client.user).has('READ_MESSAGES')).map(lc => {%>
                      <option value="<%=lc.id %>">#<%=lc.name%></option>
                      <%   
                      });
                      %>
                    </select>
                  
                  </div>
                  
                  <div class="input-field col s6">
